<h1>Update entry name</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@entry) do |f| %>
      <%= f.label :title %>
      <%= f.text_field :title, class: 'form-control' %>
      <%= f.label :text %>
      <%= f.text_field :text, class: 'form-control' %>
	  <%= f.label :year %>
      <%= f.text_field :year, class: 'form-control' %>


      <%= f.submit "Save changes" %>
    <% end %>

    
  </div>
</div>