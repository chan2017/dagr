  

<%= form_for @entry, url: {action: "create"}, html: {class: "nifty_form"} do |form| %>



  <p>
    <%= form.label :title %><br>
    <%= form.text_field :title %>
  </p>

  
 
  <p>
    <%= form.label :text %><br>
    <%= form.text_area :text %>
  </p>

  <p>
    <%= form.label :year %><br>
    <%= form.number_field :year %>
  </p>
 
  <p>
    <%= form.submit "create"%>
  </p>
<% end %>