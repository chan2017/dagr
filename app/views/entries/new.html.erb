  
<%= form_with scope: :entry, url: entries_path, local: true do |form| %>
  


  <p>
    <%= form.label :title %><br>
    <%= form.text_field :title %>
  </p>
 
  <p>
    <%= form.label :text %><br>
    <%= form.text_area :text %>
  </p>

  <p>
    <%= form.label :year %><br>
    <%= form.number_field :integer %>
  </p>
 
  <p>
    <%= form.submit "create"%>
  </p>
<% end %>